{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // Tabs
        "indent": ["error", "tab"],
        "no-tabs": ["error", {"allowIndentationTabs": true }],

        // Alignment
        "key-spacing": ["error", {"align": "value"}]
    }
}
